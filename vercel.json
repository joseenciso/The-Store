{
    "version": 2,
    "builds":[
        {
            "src": "the_store/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [      
        {
            "src": "/(.*)",
            "dest": "the_store/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "home/"
        },
    ]
}